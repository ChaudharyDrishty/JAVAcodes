import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
Scanner ob= new Scanner(System.in);
int a;char c;

c=ob.next().charAt(0);
a=(int)c;
if(a>=65&&a<=90)
System.out.println("1");
else if(a>=97&&a<=122)
System.out.println("0");
else
System.out.println("-1");
    }
}
