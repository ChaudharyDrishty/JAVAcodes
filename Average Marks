import java.util.Scanner;

public class Solution {


	public static void main(String[] args) {
		
		Scanner ob=new Scanner(System.in);
	 	String s=ob.next();
		int a=ob.nextInt();
		int b=ob.nextInt();
		int c=ob.nextInt();
		int avg=(a+b+c)/3;
		System.out.println(s);
		System.out.println(avg);
		
	}

}
